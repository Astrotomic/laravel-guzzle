{
    "name": "astrotomic/laravel-guzzle",
    "description": "Laravel wrapper for guzzlehttp/guzzle",
    "keywords": [
        "astrotomic",
        "laravel-guzzle",
        "guzzle",
        "guzzlehttp",
        "http",
        "http-client"
    ],
    "type": "library",
  "homepage": "https://astrotomic.info",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Witkowski",
      "email": "gummibeer@astrotomic.info",
      "homepage": "https://gummibeer.de",
      "role": "Developer"
    }
  ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\LaravelGuzzle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\LaravelGuzzle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrotomic\\LaravelGuzzle\\GuzzleServiceProvider"
            ]
        }
    },
      "support": {
    "email": "dev@astrotomic.info",
    "issues": "https://github.com/Astrotomic/laravel-linfo/issues",
    "source": "https://github.com/Astrotomic/laravel-linfo"
  }
}
